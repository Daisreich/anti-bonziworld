{
    "name": "Anti-BonziWORLD",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Blocks everything known to man about BonziWORLD.",
    "icons": {
      "128" : "icon_128.png"
    },
    "host_permissions": ["*://*/*"  ], 
	  "declarative_net_request" : {
		"rule_resources" : [
			{
			  "id": "no_bw",
			  "enabled": true,
			  "path": "no_bw.json"
			}
		]
	  },
    "permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestFeedback"
    ]
  }
