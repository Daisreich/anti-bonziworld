{
    "name": "Anti-Grabify",
    "version": "1.12",
    "manifest_version": 2,
    "description": "Blocks IP grabbers from Grabify and 50+ other IP grabber links.",
    "background": {
      "scripts": ["background.js"]
    },
  
    "icons": {
      "128" : "icon_128.png"
    },
    "browser_action": {
      "default_icon": "icon_128.png",
      "default_title": "Anti-Grabify",
      "default_popup": "popup.html"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ]
  }
