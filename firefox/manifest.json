{
"applications": {
  "gecko": {
    "strict_min_version": "56.0a1"
  }
},

    "manifest_version": 2,
    "name": "Anti-Grabify",
    "description": "Blocks IP grabbers from the Grabify website.",
    "version": "1.0",
  
    "icons": {
      "48": "icons/48.svg",
      "96": "icons/96.svg"
    },
  
  
    "background": {
      "scripts": [
        "background/proxy-handler.js"
      ]
    },
  

    "permissions": ["proxy", "storage"]
  
  }